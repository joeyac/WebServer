http://stackoverflow.com/questions/1639555/return-get-data-in-a-function-using-jquery
function showGetResult( name )
{
     var result = null;
     var scriptUrl = "somefile.php?name=" + name;
     $.ajax({
        url: scriptUrl,
        type: 'get',
        dataType: 'html',
        async: false,
        success: function(data) {
            result = data;
        }
     });
     return result;
}
var D = new Date((new Date(now_time)).valueOf() + 1);


function setTime(now_time){
    var D=new Date(now_time);
    D.setSeconds(D.getSeconds() + 1);
    var year = D.getFullYear();
    var month = D.getMonth() + 1;
    if (month < 10) {
    month = "0" + month;
    }
    var date = D.getDate() + 1;
    if (date < 10) {
    date = "0" + date;
    }
    var hour = D.getHours();
    if (hour < 10) {
    hour = "0" + hour;
    }
    var minute = D.getMinutes();
    if (minute < 10) {
    minute = "0" + minute;
    }
    var second = D.getSeconds();
    if (second < 10) {
    second = "0" + second;
    }
    var result = year + "-" + month + "-" + date + " " + hour + ":" + minute + ":" + second;
	document.getElementById("server-time").innerHTML = result;
	return D.toISOString();
}



